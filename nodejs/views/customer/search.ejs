<div class="container">
    <% if(!dataSp[0]) { %>
        <p style="text-align: center;">Không có sản phẩm cần tìm</p>
    <% } else { %>
    <div class="Tieu_de" style="margin-top: 10px;">
        <div>
            Tìm kiếm: <%= title %>
        </div>
    </div>
    <div class="row">
        <% dataSp.forEach((datasp) => {
            %>
                <div class="col-sm-3"><a href="/customer/san-pham/chi-tiet/<%= datasp.ID_LoaiSanPham %>/<%= datasp.ID %>" style="color: black;">
                    <div class="card" style="width: 18rem; margin-top: 10px; margin-bottom: 20px;">
                    <img src="/admin/image/<%= datasp.imgName %>" class="card-img-top" id="NgoaiNhap1" alt="..." style="max-height: 450px">
                    <div class="card-bodya">
                        <div class="card-title">
                            <h5><%= datasp.TenSanPham %></h5>
                        </div>
                        <div class="card-info">
                            <h5 class="card-made"><%= datasp.XuatXu %></h5>
                            <h5 class="card-price"><%= datasp.GiaBan %>VND</h5>
                        </div>

                    </div>
                    </div></a>
                </div>
            <%  
        }) %>
    </div>
    <% } %>
</div>